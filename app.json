{
  "expo": {
    "name": "2048",
    "slug": "2048",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/2048logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/2048logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/2048logo.png",
        "backgroundColor": "#ffffff"
      },
       "package": "com.2048.game"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/2048logo.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "40630c43-aad1-48a8-95e9-297ddd9f0a23"
      }
    }
  }
}
